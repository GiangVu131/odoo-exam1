<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="s_update_discount_code_view_form" model="ir.ui.view">
        <field name="name">s_update_warranty_view_form</field>
        <field name="model">s.update.warranty</field>
        <field name="arch" type="xml">
            <form string="s_update_warranty_form">
                <sheet>
                    <group>
                        <field name="product_ids" widget="many2many_tags" readonly="1"/>
                    </group>
                </sheet>
                <footer>
                    <field name="input_date"/>
                    <button name="update_warranty" string="Confirm" type="object"
                            class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel" data-hotkey="z"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>

